# Only Relevant Configs for Current Company
source ~/.alias.work

alias cdev="cd ~/dev"
alias cdp="cd ~/dev/python"
alias cdg="cd ~/dev/go"

alias vi="nvim"
alias vim="nvim"

alias chvim="nvim ~/.config/nvim/lua.vim"

# Don't need this line if using Linux
alias docker_start="open --hide --background -a Docker"

# Docker
function _docker_build_and_push_image(){
    docker build . -t $1 && docker push $1
}
alias dbp='_docker_build_and_push_image $1'
alias dbash='docker exec -it $1 /bin/bash'

# tmuxinator
alias txn='tmuxinator new'
# Copy a project (old, new)
alias txcp='tmuxinator copy'
# Close current project
alias txs='tmuxinator stop $(tmux display-message -p '#S')'
